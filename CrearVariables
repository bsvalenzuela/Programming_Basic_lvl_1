var age = 10; //a침os
var height = 100; //altura en centimetros
if (age >=10 && height >=100) //si la edad es mayor o igual a 10 y la altura
                              //es mayor o igual a 100, entonces se ejecuta lo siguiente
{
 console.log('la persona puede subirse a la atracci칩n')   
}
else {
    console.log('la persona no puede subirse')
}
//si la variable age es menor a 10, la persona no podra subirse a la atracci칩n
